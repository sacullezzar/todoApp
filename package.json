{
  "name": "todoapp",
  "version": "1.1.0",
  "description": "a react todo app, built from scratch",
  "main": "index.js",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --config ./webpack/webpack.dev.config.js",
    "test": "test",
    "start": "npm run build && node server.js",
    "build": "npx webpack",
    "launch": "concurrently \"cd backend && node server.js\" \"npm start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sacullezzar/todoApp.git"
  },
  "keywords": [
    "todo",
    "app",
    "react",
    "mongo",
    "express",
    "node",
    "mern"
  ],
  "author": "sacullezzar",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sacullezzar/todoApp/issues"
  },
  "homepage": "https://github.com/sacullezzar/todoApp#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "es6-promise": "^4.2.6",
    "isomorphic-fetch": "^2.2.1",
    "mongoose": "^5.5.5",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.1",
    "dotenv": "^8.0.0",
    "less": "^3.9.0",
    "less-loader": "^5.0.0",
    "react-redux": "^7.0.3",
    "redux-thunk": "^2.3.0",
    "style-loader": "^0.23.1",
    "uuid": "^3.3.2",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-server": "^3.3.1"
  }
}
